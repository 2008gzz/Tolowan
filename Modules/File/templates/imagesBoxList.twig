<div class="row">
	<div class="col-xs-12">
		<div class="row-fluid">
			<div class="alert alert-block alert-info">
				<i class="fa fa-fw fa-file-image-o"></i>
				点击图片，添加选中列表
			</div>
			{{ content() }}
			<ul class="list boxs-list">
				<li>
					<img class="images-box-item" src="/images/banner1.jpeg">
				</li>
				<li>
					<img class="images-box-item" src="/images/banner2.jpeg">
				</li>
				<li">
					<img class="images-box-item" src="/images/banner3.jpeg">
				</li>
				<li>
					<img class="images-box-item"  src="/images/banner4.jpeg">
				</li>
				<li>
					<img class="images-box-item" src="/images/banner5.jpeg">
				</li>
				{% for item in data.items %}
					<li class="{{ item.content_type }}">
						<img class="images-box-item" src="{{ item.url }}">
					</li>
				{% else %}
					<li>图库为空</li>
				{% endfor %}
			</ul>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-xs-12">
		{{ partial("Themes/AdminLTE/templates/paginator") }}
	</div>
</div>
<style type="text/css">
	.list li{
        list-style: none;
        display: inline-block;
    }
	.list li img{
		height: 100px;
		width:auto;
	}
</style>